{
  "Version": "2012-10-17",
  "Id": "Policy1472860073114",
  "Statement": [
    {
      "Sid": "Stmt1472860070369",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::766732214526:role/Fineo-BatchIngest"
      },
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::<your bucket here>",
        "arn:aws:s3:::<your bucket here>/<key>"
        ]
    }
  ]
}
